{
  "version": "2.0.0",
  "app_version": "KakaoT v5.2.3",
  "last_updated": "2026-01-06",

  "view_ids": {
    "reserved_at": "com.kakao.taxi.driver:id/tv_reserved_at",
    "path": "com.kakao.taxi.driver:id/tv_path",
    "fare": "com.kakao.taxi.driver:id/tv_fare",
    "stopovers": "com.kakao.taxi.driver:id/tv_stopovers",
    "surge": "com.kakao.taxi.driver:id/tv_surge",
    "item_container": "com.kakao.taxi.driver:id/vg_item"
  },

  "patterns": {
    "price": {
      "regex": "(\\d{1,3}(,\\d{3})*|\\d+)\\s*원",
      "description": "금액 패턴 (예: 15,000원, 15000원)"
    },
    "time": {
      "regex": "\\d{2}\\.\\d{2}\\([^)]+\\)\\s+\\d{2}:\\d{2}.*",
      "description": "예약 시간 패턴 (예: 01.10(목) 14:30)"
    },
    "route": {
      "regex": "(.+)\\s*→\\s*(.+)",
      "description": "경로 패턴 (출발지 → 도착지)"
    }
  },

  "validation": {
    "price_min": 2000,
    "price_max": 300000,
    "location_min_length": 2,
    "reservation_time_required": true
  },

  "parsing": {
    "min_text_count": 2,
    "enable_view_id_strategy": true,
    "enable_regex_strategy": true,
    "enable_heuristic_strategy": true
  }
}
